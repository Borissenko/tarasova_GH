{
  "name": "starter-apollo-server",
  "version": "1.0.0",
  "description": "Starter Apollo-server",
  "main": "server_2.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\"",
    "inst": "npm install && cd client && npm install"
  },
  "dependencies": {
    "apollo-server": "^2.0.0-rc.7",
    "graphql": "^0.13.2"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "nodemon": "^1.18.1"
  }
}
